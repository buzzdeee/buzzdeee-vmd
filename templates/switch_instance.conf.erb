# This file is maintained via Puppet

switch "<%= @title %>" {
  add <%= @interface %>
  <%- if @enable -%>
  enable
  <%- else -%>
  disable
  <%- end -%>
  <%- if @group -%>
  group <%= @group %>
  <%- end -%>
  <%- if @locked_lladdr -%>
  locked lladdr
  <%- end -%>
  <%- if @rdomain -%>
  rdomain <%= @rdomain %>
  <%- end -%>
  <%- if @forwarding -%>
  up
  <%- else -%>
  down
  <%- end -%>

}
